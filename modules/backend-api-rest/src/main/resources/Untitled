            						var fix = $('#IsFixedRoadTransport').val();
            						var contract = $('#IsContractRoadTransport').val();
            						var passenger = $('#IsPassengerTransport').val();
            						var cargo = $('#IsCargoTransport').val();

            						if(fix == '0'){
            							$('#line1').hide();
            						}
            						if(contract == '0'){
            							$('#line2').hide();
            						}
            						if(passenger == '0'){
            							$('#line3').hide();
            						}
            						if(cargo == '0'){
            							$('#line4').hide();
            						}


